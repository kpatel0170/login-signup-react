{
  "name": "nextgen-mern",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "bin/www.js",
    "start:dev": "node -r ./src/config/tracing.cjs --watch bin/www.js",
    "dev": "NODE_ENV=development nodemon --experimental-modules --trace-warnings bin/www.js",
    "pre-commit": "eslint . --fix && prettier --write '**/**/*.js'",
    "build": "rm -rf build && prettier --write src/ ",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix --ext .",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "prepare": "husky install || true",
    "compose:up": "docker-compose --profile o11y up",
    "compose:down": "docker-compose down -v",
    "compose:remove": "docker-compose down -v --rmi all"
  },
  "lint-staged": {
    "**/**/*.js": "eslint --fix",
    "**/**/*.{js,json,md,yaml,yml,pug}": "prettier --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/mongoose": "^3.0.1",
    "@godaddy/terminus": "^4.12.1",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "adminjs": "^6.8.2",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-prom-bundle": "^6.6.0",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "http": "^0.0.1-security",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  }
}
