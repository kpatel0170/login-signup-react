{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:security/recommended", "plugin:prettier/recommended"],
    "plugins": ["security", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "security/detect-object-injection": "off"
    }
}
